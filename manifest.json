{
  "name": "Auto Login",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "자동로그인",
  "homepage_url": "https://h2ck4u.github.io",
  "browser_action": {
    "default_popup": "index.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://*.synap.co.kr/*"],
      "js": ["contentScript.js", "jquery-1.11.0.min.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
      "activeTab", "http://*.synap.co.kr/*", "https://*.synap.co.kr/*"
  ]
}